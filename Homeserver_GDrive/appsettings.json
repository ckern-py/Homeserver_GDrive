{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AppSettings": {
    "DisplayName": "Penthouse_GDrive",
    "DetailErrorRecordsCount": 10,
    "DetailInfoRecordsCount": 20,
    "DetailUploadRecordsCount": 15,
    "DetailDownloadRecordsCount": 15,
    "TopLevelGDriveUploadFolder": "HomeServer_Upload",
    "TopLevelGDriveDownloadFolder": "HomeServer_Download",
    "GoogleApplicationName": "penthouse-gdrive",
    "DBMaintenanceDelayHours": 4,
    "UploadServiceDelayHours": 8,
    "DownloadServiceDelayHours": 12
  }
}
